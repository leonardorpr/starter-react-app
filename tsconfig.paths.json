{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "components": [
        "app/components"
      ],
      "components/*": [
        "app/components/*"
      ],
      "routes": [
        "app/routes"
      ],
      "routes/*": [
        "app/routes/*"
      ],
      "screens": [
        "app/screens",
      ],
      "screens/*": [
        "app/screens/*"
      ],
      "assets": [
        "assets"
      ],
      "assets/*": [
        "assets/*"
      ],
      "configs": [
        "configs"
      ],
      "configs/*": [
        "configs/*"
      ],
      "forms": [
        "core/forms"
      ],
      "forms/*": [
        "core/forms/*"
      ],
      "hooks": [
        "core/hooks"
      ],
      "hooks/*": [
        "core/hooks/*"
      ],
      "interfaces": [
        "core/interfaces"
      ],
      "interfaces/*": [
        "core/interfaces/*"
      ],
      "services": [
        "core/services"
      ],
      "services/*": [
        "core/services/*"
      ],
      "api": [
        "core/services/api"
      ],
      "api/*": [
        "core/services/api/*"
      ],
      "store": [
        "core/store"
      ],
      "store/*": [
        "core/store/*"
      ],
      "sagas": [
        "core/store/sagas"
      ],
      "sagas/*": [
        "core/store/sagas/*"
      ],
      "slices": [
        "core/store/slices"
      ],
      "slices/*": [
        "core/store/slices/*"
      ],
      "utils": [
        "utils"
      ],
      "utils/*": [
        "utils/*"
      ],
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "src"
  ]
}
